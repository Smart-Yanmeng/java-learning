<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.york.websocket.pojo.IMessageMapper">

    <insert id="saveMessage" parameterType="com.york.websocket.pojo.Message">
        insert into message(to_name, message)
        values (#{toName}, #{message})
    </insert>

</mapper>
